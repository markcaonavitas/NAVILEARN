                                <StackLayout.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="SelectForDataLogging" NumberOfTapsRequired="2"  />
                                </StackLayout.GestureRecognizers>
                                <StackLayout Orientation="Horizontal">
                                    <Label Text="{Binding Path=Name,  StringFormat='{0}'}" TextColor="Black" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                                    <Label Text="{Binding Path=parameterValueString,  StringFormat='{0}'}" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalOptions="EndAndExpand" HorizontalTextAlignment="End"/>
                                </StackLayout>
                            </StackLayout>
                        </Frame>
                        <Frame OutlineColor="Black" Padding="5,1,5,1" Margin="5,1,5,1">
                            <StackLayout Orientation="Vertical">
                                <StackLayout Orientation="Horizontal" x:Name="ENCODERA">
                                    <StackLayout.GestureRecognizers>
                                        <TapGestureRecognizer Tapped="SelectForDataLogging" NumberOfTapsRequired="2"  />
                                    </StackLayout.GestureRecognizers>
                                    <Label Text="{Binding Path=Name,  StringFormat='{0}'}" TextColor="Black" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                                    <Label Text="{Binding Path=parameterEnumString,  StringFormat='{0}'}" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalOptions="EndAndExpand" HorizontalTextAlignment="End"/>
                                </StackLayout>
                                <StackLayout Orientation="Horizontal"  x:Name="ENCODERB">
                                    <StackLayout.GestureRecognizers>
                                        <TapGestureRecognizer Tapped="SelectForDataLogging" NumberOfTapsRequired="2"  />
                                    </StackLayout.GestureRecognizers>
                                    <Label Text="{Binding Path=Name,  StringFormat='{0}'}" TextColor="Black" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                                    <Label Text="{Binding Path=parameterEnumString,  StringFormat='{0}'}" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalOptions="EndAndExpand" HorizontalTextAlignment="End"/>
                                </StackLayout>
                            </StackLayout>
                        </Frame>
                    </StackLayout>
                    <StackLayout Orientation="Vertical">
                        <StackLayout Orientation="Horizontal">
                            <Image IsVisible="True" Source="Information_icon.png" Margin="0" Scale=".6" HorizontalOptions="Start"/>
                            <Label  Text="On The Fly Programmer (OTF)" TextColor="#367F56"   FontSize="Default"  VerticalOptions="Center" HorizontalOptions="Start" HorizontalTextAlignment="Start"/>
                            <Image IsVisible="False" Source="collapseIcon.png" Margin="0" Scale=".6" HorizontalOptions="Start"/>
                            <StackLayout.GestureRecognizers>
                                <TapGestureRecognizer Tapped="FillInDescriptions" NumberOfTapsRequired="1"/>
                            </StackLayout.GestureRecognizers>
                        </StackLayout>
                        <StackLayout Orientation="Vertical" IsVisible="False">
                            <Label TextColor="#367F56" FontSize="Default"  VerticalOptions="Center" HorizontalOptions="FillAndExpand" HorizontalTextAlignment="Start"
                            Text="Navitas optionally supplies a dashboard mounted, 3 dial performance programmer. We cannot automatically detect if this device is installed or not">
                            </Label>
                        </StackLayout>
                        <Frame OutlineColor="Black" Padding="5,1,5,1" Margin="5,1,5,1">
                            <StackLayout Orientation="Vertical" x:Name="PROGIN">
                                <StackLayout.GestureRecognizers>
                                    <TapGestureRecognizer Tapped="SelectForDataLogging" NumberOfTapsRequired="2"  />
                                </StackLayout.GestureRecognizers>
                                <StackLayout Orientation="Horizontal">
                                    <Label Text="{Binding Path=Name,  StringFormat='{0}'}"  TextColor="Black"   FontSize="Default" VerticalOptions="Center" HorizontalOptions="FillAndExpand" HorizontalTextAlignment="Start" >
                                    </Label>
                                    <Label Text="{Binding Path=parameterEnumString, StringFormat='{0}'}" TextColor="Gray" FontSize="Micro" VerticalOptions="Center" HorizontalOptions="EndAndExpand"  HorizontalTextAlignment="End"/>
                                </StackLayout>
                            </StackLayout>
                        </Frame>
                        <Frame OutlineColor="Black" Padding="5,1,5,1" Margin="5,1,5,1">
                            <StackLayout Orientation="Vertical">
                                <StackLayout Orientation="Horizontal" x:Name="OTFPROG1MAX">
                                    <StackLayout.GestureRecognizers>
                                        <TapGestureRecognizer Tapped="SelectForDataLogging" NumberOfTapsRequired="2"  />
                                    </StackLayout.GestureRecognizers>
                                    <Label Text="{Binding Path=Name,  StringFormat='{0}'}" TextColor="Black" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                                    <Label Text="{Binding Path=parameterValue,  StringFormat='{0:F0}'}" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalOptions="EndAndExpand" HorizontalTextAlignment="End"/>
                                </StackLayout>
                                <StackLayout Orientation="Horizontal"  x:Name="OTFPROG2MAX">
                                    <StackLayout.GestureRecognizers>
