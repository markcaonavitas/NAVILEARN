                                    <Label Text="{Binding Path=parameterValue,  StringFormat='{0:F0}'}" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalOptions="EndAndExpand" HorizontalTextAlignment="End"/>
                                </StackLayout>
