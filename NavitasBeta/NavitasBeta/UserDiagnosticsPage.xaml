                                    <Label Text="Phone App Version Number" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                                    <Label Text="{Binding Path=parameterValueString,  StringFormat='{0:0.000}'}" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalOptions="EndAndExpand" HorizontalTextAlignment="End"/>
                                </StackLayout>
                            </StackLayout>
                        </Frame>
                    </StackLayout>
                    <Label x:Name="RISERPMSEC" Text="just to get value" IsVisible="False" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                    <Label x:Name="ScalerForMuchFasterNonVehicleAccelAndDecel_PUq0" Text="just to get value" IsVisible="False" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                    <Label x:Name="FORWARDSPEED" Text="just to get value" IsVisible="False" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                    <Label x:Name="RISEHZSEC" Text="just to get value" IsVisible="False" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                    <Label x:Name="FALLHZSEC" Text="just to get value" IsVisible="False" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                    <Label x:Name="FWDLMTRPM" Text="just to get value" IsVisible="False" TextColor="Gray" FontSize="Default" VerticalOptions="Center" HorizontalTextAlignment="Start"/>
                    <!--crazy binding redirection for now
                    and note a Stepper.Value is a double but it has a automatic converter for boolean
                    but when we use out converterInverter we have to change a double to a boolean
                    (TODO: obviously needs more thought)-->
                    <Stepper x:Name="DEMOMODE" Value="{Binding Path=parameterBoolean,Mode=OneWay}" IsVisible="False"/>
                </StackLayout>
            </ScrollView>
            <ContentView x:Name="popupDiagnostics"
                 AbsoluteLayout.LayoutBounds="0, 0, 1, 1"
                 AbsoluteLayout.LayoutFlags="All"
                 IsVisible="False"
                 BackgroundColor="#80808080"
                 Padding="10, 0">
            </ContentView>
        </AbsoluteLayout>
